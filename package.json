{
  "name": "ai-survey",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "concurrently -n web,worker -c cyan,yellow \"next dev -p 5001\" \"tsx src/worker/index.ts\"",
    "dev:web": "next dev -p 5001",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "worker": "tsx src/worker/index.ts",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:push": "prisma db push",
    "seed": "tsx prisma/seed.ts",
    "test": "vitest run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.78.0",
    "@prisma/client": "^6.19.2",
    "bcryptjs": "^3.0.3",
    "compromise": "^14.15.0",
    "jose": "^6.1.3",
    "next": "^16.1.6",
    "openai": "^6.25.0",
    "prisma": "^6.19.2",
    "react": "^19.2.4",
    "react-dom": "^19.2.4",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.2.1",
    "@types/bcryptjs": "^3.0.0",
    "@types/node": "^25.3.2",
    "@types/react": "^19.2.14",
    "@types/react-dom": "^19.2.3",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.3",
    "eslint-config-next": "^16.1.6",
    "postcss": "^8.5.6",
    "tailwindcss": "^4.2.1",
    "tsx": "^4.21.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.18"
  }
}
